<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daleel.ghazeihdaleel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_daleel"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_daleel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.daleel.ghazeihdaleel.Medical"></activity>
        <activity android:name="com.daleel.ghazeihdaleel.AddContact" />
        <activity android:name="com.daleel.ghazeihdaleel.Emergency" />
        <activity
            android:name="com.daleel.ghazeihdaleel.showContactDetails"
            android:screenOrientation="portrait" />
        <activity android:name="com.daleel.ghazeihdaleel.list" />
        <activity android:name="com.daleel.ghazeihdaleel.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.daleel.ghazeihdaleel.MainActivity" />
    </application>

</manifest>